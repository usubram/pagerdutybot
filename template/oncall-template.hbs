{{/each}}
{{#each result}}
	{{#if summary}}
		*Escalation: {{name}}* \n
		{{#each oncall}}
			{{#if level}}
			*Level {{level}}* \n
			{{/if}}
			```
			Name: {{name}} \n
			{{#each contact_methods}}
				{{#if label}}
					{{label}}: {{#if country_code}} +{{country_code}}{{/if}} {{address}} \n
				{{/if}}
			{{/each}}
			{{#if job_title}}
				Title: {{job_title}} \n
			{{/if}}
			``` \n
		{{/each}}
	{{/if}}
{{/each}}
{{#if result.not_found}}
